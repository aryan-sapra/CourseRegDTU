{
  "name": "coureregdtu",
  "version": "1.0.0",
  "description": "Course Registrar DTU",
  "main": "main.js",
  "author": {
    "name": "potui",
    "email": "potui@potui.xyz"
  },
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "dist:win": "electron-builder --win",
    "dist:mac": "electron-builder --mac",
    "dist:linux": "electron-builder --linux"
  },
  "build": {
    "appId": "com.saksham.coureregdtu",
    "productName": "CourseRegistrar",
    "directories": {
      "output": "dist"
    },
    "files": ["**/*"],
    "win": {
      "target": ["nsis", "zip"],
      "icon": "assets/dtulogo.ico"
    },
    "mac": {
      "target": ["dmg", "zip"],
      "icon": "assets/dtulogo.icns"
    },
    "linux": {
      "target": ["AppImage", "deb"],
      "icon": "assets/dtulogo.png",
      "category": "Utility"
    }
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "got": "^14.4.5",
    "node-notifier": "^10.0.1"
  },
  "devDependencies": {
    "electron": "^34.5.8",
    "electron-builder": "^24.6.0"
  }
}